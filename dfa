digraph finite_state_machine {
	rankdir=LR size="8,5"
	node [shape=point]
	""
	node [shape=circle]
	"" -> A [label=""]
	node [shape=doublecircle]
	C
	B
	node [shape=circle]
	A -> B [label=a]
	A -> C [label=b]
	B -> D [label=a]
	B -> D [label=b]
	C -> E [label=a]
	C -> E [label=b]
	D -> D [label=a]
	D -> E [label=b]
	E -> A [label=a]
	E -> E [label=b]
}
